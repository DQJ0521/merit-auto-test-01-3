    isVip: 0
    positionInfoId: "19"
    remark: "自动化测试"
    seq: 999
    tags: [ "16" ]
    title: "自动化测试数据"
    # 是否有依赖业务，为空或者false则表示没有
  dependence_case: True
  # 依赖的数据
  dependence_case_data:
    - case_id:
      dependent_data:
        - dependent_type:
          jsonpath:
          replace_key:
        - dependent_type:
          jsonpath:
          replace_key:
