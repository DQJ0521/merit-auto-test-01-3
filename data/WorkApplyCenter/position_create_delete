    Content-Type: application/json;charset=UTF-8
    Authorization: work_login_init_houtai
  # 请求的数据，是 params 还是 json、或者file
  requestType: json
  # 是否执行，空或者 true 都会执行
  is_run: true
  data:
    beginTime: "2022-06-20 14:30:15"
    content: "自动化测试自动化测试"
    contentType: "app_link"
    endTime: "2023-06-30 14:30:26"
